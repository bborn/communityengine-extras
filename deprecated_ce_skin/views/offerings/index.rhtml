<h1>Listing offerings</h1>

<table>
  <tr>
  </tr>
  
<% for offering in @offerings %>
  <tr>
    <td><%= link_to 'Show', user_offering_path(offering) %></td>
    <td><%= link_to 'Edit', edit_user_offering_path(offering) %></td>
    <td><%= link_to 'Destroy', user_offering_path(offering), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New offering', new_user_offering_path %>